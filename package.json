{
  "name": "@shacl-form-editor/shacl-form-editor",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint . --fix --cache",
    "format": "prettier --write \"**/*.{js,ts,json,vue,md}\"",
    "docker:up": "docker-compose -f deploy/docker-compose.yml up -d",
    "docker:down": "docker-compose -f deploy/docker-compose.yml down",
    "docker:rebuild": "docker-compose -f deploy/docker-compose.yml up -d --build"
  },
  "devDependencies": {
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "~10.5.0",
    "prettier": "3.6.2"
  }
}
